[run]
source = client,server
omit =
    */tests/*
    */test_*.py
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

show_missing = True
precision = 2

[html]
directory = htmlcov
